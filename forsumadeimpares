#include <iostream>

using namespace std;

int main()
{
    int suma=0,n=0;
    cout<<"ingrese hasta que numero desea sumar"<<endl;
    cin>>n;
    for(int i=1;i<=n;i++){
        if(i%2!=0){
            suma=suma+i;
        }
        
    }
    cout<<"la suma de los impares es "<<suma<<endl;

    return 0;
}
